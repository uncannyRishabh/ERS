<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.ExamRegistration" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="background">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowContentTransitions">true</item>

        <item name="android:navigationBarColor">#00000000</item>

        <item name="android:enforceNavigationBarContrast">false</item>
    </style>

    <style name="Splash" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:statusBarColor">@color/purple_700</item>
        <item name="android:windowBackground">@drawable/splashh</item>
        <item name="android:navigationBarColor">#5600E8</item>
    </style>

    <style name="navbarColorDark" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:statusBarColor">@color/purple_700</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:navigationBarColor">#2B2B2B</item>
    </style>

    <style name="navbarColor_ActionBar" parent="Theme.MaterialComponents.Light">
        <item name="android:navigationBarColor">#ffffff</item>
        <item name="actionBarTheme">@style/Theme.AppCompat.DayNight.DarkActionBar</item>
    </style>

</resources>